<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	   xmlns:tns="http://webservices.amazon.com/AWSECommerceService/2011-08-01" 
	   targetNamespace="http://webservices.amazon.com/AWSECommerceService/2011-08-01" elementFormDefault="qualified">
  <xsd:element name="Bin">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="BinName" type="xsd:string"/>
	<xsd:element name="BinItemCount" type="xsd:positiveInteger"/>
	<xsd:element name="BinParameter" minOccurs="0" maxOccurs="unbounded">
	  <xsd:complexType>
	    <xsd:sequence>
	      <xsd:element name="Name" type="xsd:string"/>
	      <xsd:element name="Value" type="xsd:string"/>
	    </xsd:sequence>
	  </xsd:complexType>
	</xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SearchBinSet">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:Bin" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="NarrowBy" type="xsd:string" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SearchBinSets">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:SearchBinSet" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ItemSearch">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:ItemSearchRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:ItemSearchRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="ItemSearchRequest">
    <xsd:sequence>
      <xsd:element name="Actor" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Artist" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Availability" minOccurs="0">
	<xsd:simpleType>
	  <xsd:restriction base="xsd:string">
	    <xsd:enumeration value="Available"/>
	  </xsd:restriction>
	</xsd:simpleType>
      </xsd:element>
      <xsd:element ref="tns:AudienceRating" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="Author" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Brand" type="xsd:string" minOccurs="0"/>
      <xsd:element name="BrowseNode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Composer" type="xsd:string" minOccurs="0"/>
      <xsd:element ref="tns:Condition" minOccurs="0"/>
      <xsd:element name="Conductor" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Director" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ItemPage" type="xsd:positiveInteger" minOccurs="0"/>
      <xsd:element name="Keywords" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Manufacturer" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MaximumPrice" type="xsd:nonNegativeInteger" minOccurs="0"/>
      <xsd:element name="MerchantId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MinimumPrice" type="xsd:nonNegativeInteger" minOccurs="0"/>
      <xsd:element name="MinPercentageOff" type="xsd:nonNegativeInteger" minOccurs="0"/>
      <xsd:element name="MusicLabel" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Orchestra" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Power" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Publisher" type="xsd:string" minOccurs="0"/>
      <xsd:element name="RelatedItemPage" type="tns:positiveIntegerOrAll" minOccurs="0"/>
      <xsd:element name="RelationshipType" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="SearchIndex" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Sort" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Title" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ReleaseDate" type="xsd:string" minOccurs="0"/>
      <xsd:element name="IncludeReviewsSummary" type="xsd:string" minOccurs="0"/>
      <xsd:element name="TruncateReviewsAt" type="xsd:nonNegativeInteger" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="ItemLookup">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:ItemLookupRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:ItemLookupRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="ItemLookupRequest">
    <xsd:sequence>
      <xsd:element ref="tns:Condition" minOccurs="0"/>
      <xsd:element name="IdType" minOccurs="0">
	<xsd:simpleType>
	  <xsd:restriction base="xsd:string">
	    <xsd:enumeration value="ASIN"/>
	    <xsd:enumeration value="UPC"/>
	    <xsd:enumeration value="SKU"/>
	    <xsd:enumeration value="EAN"/>
	    <xsd:enumeration value="ISBN"/>
	  </xsd:restriction>
	</xsd:simpleType>
      </xsd:element>
      <xsd:element name="MerchantId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ItemId" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="SearchIndex" type="xsd:string" minOccurs="0"/>
      <xsd:element name="VariationPage" type="tns:positiveIntegerOrAll" minOccurs="0"/>
      <xsd:element name="RelatedItemPage" type="tns:positiveIntegerOrAll" minOccurs="0"/>
      <xsd:element name="RelationshipType" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="IncludeReviewsSummary" type="xsd:string" minOccurs="0"/>
      <xsd:element name="TruncateReviewsAt" type="xsd:nonNegativeInteger" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="SimilarityLookup">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:SimilarityLookupRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:SimilarityLookupRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="SimilarityLookupRequest">
    <xsd:sequence>
      <xsd:element ref="tns:Condition" minOccurs="0"/>
      <xsd:element name="ItemId" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="MerchantId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="SimilarityType" minOccurs="0">
	<xsd:simpleType>
	  <xsd:restriction base="xsd:string">
	    <xsd:enumeration value="Intersection"/>
	    <xsd:enumeration value="Random"/>
	  </xsd:restriction>
	</xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="CartGet">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:CartGetRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:CartGetRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CartGetRequest">
    <xsd:sequence>
      <xsd:element name="CartId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="HMAC" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MergeCart" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="CartAdd">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:CartAddRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:CartAddRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CartAddRequest">
    <xsd:sequence>
      <xsd:element name="CartId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="HMAC" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MergeCart" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Items" minOccurs="0">
	<xsd:complexType>
	  <xsd:sequence>
	    <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
	      <xsd:complexType>
		<xsd:sequence>
		  <xsd:element name="ASIN" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="OfferListingId" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="Quantity" type="xsd:positiveInteger" minOccurs="0"/>
		  <xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="ListItemId" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	      </xsd:complexType>
	    </xsd:element>
	  </xsd:sequence>
	</xsd:complexType>
      </xsd:element>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="CartCreate">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:CartCreateRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:CartCreateRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CartCreateRequest">
    <xsd:sequence>
      <xsd:element name="MergeCart" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Items" minOccurs="0">
	<xsd:complexType>
	  <xsd:sequence>
	    <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
	      <xsd:complexType>
		<xsd:sequence>
		  <xsd:element name="ASIN" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="OfferListingId" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="Quantity" type="xsd:positiveInteger" minOccurs="0"/>
		  <xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="ListItemId" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="MetaData" minOccurs="0" maxOccurs="unbounded">
		    <xsd:complexType>
		      <xsd:sequence>
			<xsd:element name="Key" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Value" type="xsd:string" minOccurs="0"/>
		      </xsd:sequence>
		    </xsd:complexType>
		  </xsd:element>
		</xsd:sequence>
	      </xsd:complexType>
	    </xsd:element>
	  </xsd:sequence>
	</xsd:complexType>
      </xsd:element>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="CartModify">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:CartModifyRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:CartModifyRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CartModifyRequest">
    <xsd:sequence>
      <xsd:element name="CartId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="HMAC" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MergeCart" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Items" minOccurs="0">
	<xsd:complexType>
	  <xsd:sequence>
	    <xsd:element name="Item" minOccurs="0" maxOccurs="unbounded">
	      <xsd:complexType>
		<xsd:sequence>
		  <xsd:element name="Action" minOccurs="0">
		    <xsd:simpleType>
		      <xsd:restriction base="xsd:string">
			<xsd:enumeration value="MoveToCart"/>
			<xsd:enumeration value="SaveForLater"/>
		      </xsd:restriction>
		    </xsd:simpleType>
		  </xsd:element>
		  <xsd:element name="CartItemId" type="xsd:string" minOccurs="0"/>
		  <xsd:element name="Quantity" type="xsd:nonNegativeInteger" minOccurs="0"/>
		</xsd:sequence>
	      </xsd:complexType>
	    </xsd:element>
	  </xsd:sequence>
	</xsd:complexType>
      </xsd:element>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="CartClear">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:CartClearRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:CartClearRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="CartClearRequest">
    <xsd:sequence>
      <xsd:element name="CartId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="HMAC" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MergeCart" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="BrowseNodeLookup">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="MarketplaceDomain" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AWSAccessKeyId" type="xsd:string" minOccurs="0"/>
	<xsd:element name="AssociateTag" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Validate" type="xsd:string" minOccurs="0"/>
	<xsd:element name="XMLEscaping" type="xsd:string" minOccurs="0"/>
	<xsd:element name="Shared" type="tns:BrowseNodeLookupRequest" minOccurs="0"/>
	<xsd:element name="Request" type="tns:BrowseNodeLookupRequest" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="BrowseNodeLookupRequest">
    <xsd:sequence>
      <xsd:element name="BrowseNodeId" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ResponseGroup" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="Condition">
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
	<xsd:enumeration value="All"/>
	<xsd:enumeration value="New"/>
	<xsd:enumeration value="Used"/>
	<xsd:enumeration value="Collectible"/>
	<xsd:enumeration value="Refurbished"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AudienceRating">
    <xsd:simpleType>
      <xsd:restriction base="xsd:string">
	<xsd:enumeration value="G"/>
	<xsd:enumeration value="PG"/>
	<xsd:enumeration value="PG-13"/>
	<xsd:enumeration value="R"/>
	<xsd:enumeration value="NC-17"/>
	<xsd:enumeration value="NR"/>
	<xsd:enumeration value="Unrated"/>
	<xsd:enumeration value="6"/>
	<xsd:enumeration value="12"/>
	<xsd:enumeration value="16"/>
	<xsd:enumeration value="18"/>
	<xsd:enumeration value="FamilyViewing"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ItemSearchResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Items" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ItemLookupResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Items" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SimilarityLookupResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Items" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CartGetResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Cart" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CartAddResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Cart" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CartCreateResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Cart" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CartModifyResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Cart" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CartClearResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:Cart" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BrowseNodeLookupResponse">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:OperationRequest" minOccurs="0"/>
	<xsd:element ref="tns:BrowseNodes" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="OperationRequest">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element ref="tns:HTTPHeaders" minOccurs="0"/>
	<xsd:element name="RequestId" type="xsd:string" minOccurs="0"/>
	<xsd:element ref="tns:Arguments" minOccurs="0"/>
	<xsd:element ref="tns:Errors" minOccurs="0"/>
	<xsd:element name="RequestProcessingTime" type="xsd:float" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Request">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="IsValid" type="xsd:string" minOccurs="0"/>
	<xsd:element name="BrowseNodeLookupRequest" type="tns:BrowseNodeLookupRequest" minOccurs="0"/>
	<xsd:element name="ItemSearchRequest" type="tns:ItemSearchRequest" minOccurs="0"/>
	<xsd:element name="ItemLookupRequest" type="tns:ItemLookupRequest" minOccurs="0"/>
	<xsd:element name="SimilarityLookupRequest" type="tns:SimilarityLookupRequest" minOccurs="0"/>
	<xsd:element name="CartGetRequest" type="tns:CartGetRequest" minOccurs="0"/>
	<xsd:element name="CartAddRequest" type="tns:CartAddRequest" minOccurs="0"/>
	<xsd:element name="CartCreateRequest" type="tns:CartCreateRequest" minOccurs="0"/>
	<xsd:element name="CartModifyRequest" type="tns:CartModifyRequest" minOccurs="0"/>
	<xsd:element name="CartClearRequest" type="tns:CartClearRequest" minOccurs="0"/>
	<xsd:element ref="tns:Errors" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Arguments">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="Argument" maxOccurs="unbounded">
	  <xsd:complexType>
	    <xsd:attribute name="Name" type="xsd:string" use="required"/>
	    <xsd:attribute name="Value" type="xsd:string"/>
	  </xsd:complexType>
	</xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="HTTPHeaders">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="Header" minOccurs="0" maxOccurs="unbounded">
	  <xsd:complexType>
	    <xsd:attribute name="Name" type="xsd:string" use="required"/>
	    <xsd:attribute name="Value" type="xsd:string" use="required"/>
	  </xsd:complexType>
	</xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Errors">
    <xsd:complexType>
      <xsd:sequence>
	<xsd:element name="Error" maxOccurs="unbounded">
	  <xsd:complexType>
	    <xsd:sequence>
	      <xsd:element name="Code" type="xsd:string"/>
	      <xsd:element name="Message" type="xsd:string"/>
	    </xsd:sequence>
	  </xsd:complexType>
	</xsd:element>
      </xsd:sequence>
